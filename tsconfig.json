{
  "compilerOptions": {
    "target": "ES2017",
    "module": "ES2015",
    "jsx": "preserve",
    "allowJs": true,
    "noEmit": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {},
    "rootDirs": [
      "src"
    ],
    "typeRoots": [],
    "types": [],
    "allowSyntheticDefaultImports": true
  }
}
