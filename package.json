{
  "name": "instantsearch-devtools",
  "version": "1.0.0",
  "description": "DevTools for the InstantSearch libraries",
  "scripts": {
    "clean": "rm -rf dist",
    "prepare": "mkdir -p dist",
    "start:extension": "yarn clean && yarn prepare && yarn manifest && webpack-dev-server --inline --no-info",
    "start:example": "cd example && yarn start",
    "build": "yarn clean && yarn prepare && yarn manifest && NODE_ENV=production webpack",
    "manifest": "cp src/manifest.json dist/manifest.json",
    "test": "tsc --noEmit && jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samouss/react-webpack-starter.git"
  },
