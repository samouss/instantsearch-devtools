{
  "name": "instantsearch-devtools",
  "version": "1.0.0",
  "description": "DevTools for the InstantSearch libraries",
  "scripts": {
    "clean": "rm -rf dist",
    "prepare": "mkdir -p dist",
    "start:iframe": "webpack-dev-server --config=src/shells/iframe/webpack.config.js",
    "start:extension:application": "yarn clean && yarn prepare && yarn manifest && webpack-dev-server --inline --no-info --config=src/shells/extension/webpack.config.js",
    "start:extension:example": "webpack-dev-server --config=src/example/webpack.config.js",
    "build": "yarn clean && yarn prepare && yarn manifest && NODE_ENV=production webpack --config=src/shells/extension/webpack.config.js",
    "manifest": "cp src/shells/extension/manifest.json dist/manifest.json",
    "test": "yarn tsc && jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samouss/react-webpack-starter.git"
  },
  "author": [
    "Samuel Vaillant <samuel.vllnt@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.0.0-beta.44",
